<div class="page-header">
  <h1>PickemSports</h1>
</div>
<table class="table table-striped table-bordered table-hover table-condensed">
    <tr>
      <th>Abbreviation</th>
      <th>Team</th>
      <th>Mascot</th>
      <th>Logo</th>
    </tr>
    <tr ng-repeat="nbateam in nbateams">
      <td>{{ nbateam.abbr }}</td>
      <td>{{ nbateam.team }}</td>
      <td>{{ nbateam.mascot }}</td>
      <td>{{ nbateam.logo }}</td>
    </tr>
  </table>
