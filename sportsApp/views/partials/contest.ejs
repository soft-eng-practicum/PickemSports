<div class="page-header">
  <h1>PickemSports</h1>
</div>

<table class="table table-striped table-bordered table-hover table-condensed">
  <tr>
    <th>Home Team</th>
    <th>Away Team</th>
    <th>Winning Team</th>
  <tr ng-repeat="matchup in contest">
    <td ng-repeat="nbaTeams in matchup.nbaTeams">
      <input type="radio" ng-model="matchup.selectedTeam" name="matchup{{matchup.matchupId}}"
      ng-value="nbaTeams.team" ng-disabled="isAvailable() === true" />{{nbaTeams.team}} : {{nbaTeams.teamScore}}
    </td>
    <td>
      {{matchup.matchupWinner}}
    </td>
  </tr>
</table>

<div ng-repeat="matchup in contest">
  <p>{{matchup.selectedTeam}}</p>
</div>
