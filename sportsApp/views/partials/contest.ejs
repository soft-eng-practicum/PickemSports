<div class="page-header">
  <h1>PickemSports</h1>
</div>

<table class="table table-striped table-bordered table-hover table-condensed">
<tr>
  <th>Home Team</th>
  <th>Away Team</th>
</tr>
<tr ng-repeat="matchup in contest">
  <td>
    <label class="radio-inline">
      <input type="radio" name="{{matchup.id}}" id="{{matchup.id}}" value="Home">{{matchup.homeTeam}}
    </label>
  </td>
  <td>
    <label class="radio-inline">
      <input type="radio" name="{{matchup.id}}" id="{{matchup.id}}" value="Away">{{matchup.awayTeam}}
    </label>
  </td>
</table>

