<section class="mbr-navbar mbr-navbar--freeze mbr-navbar--absolute mbr-navbar--transparent mbr-navbar--sticky mbr-navbar--auto-collapse
mbr-navbar--sticky mbr-navbar--auto-collapse" id="menu-0">
<div class="mbr-navbar__section mbr-section">
  <div class="mbr-section__container container">
    <div class="mbr-navbar__container">
      <div class="mbr-navbar__column mbr-navbar__column--s mbr-navbar__brand">
        <span class="mbr-navbar__brand-link mbr-brand mbr-brand--inline">
          <span class="mbr-brand__logo">
            <a ui-sref="dashboard">
              <img src="stylesheets/images/logo-340x280-1.jpg" class="mbr-navbar__brand-img mbr-brand__img">
            </a>
          </span>
          <span class="mbr-brand__name">
            <a class="mbr-brand__name text-white" ui-sref="dashboard">PickemSports
            </a>
          </span>
        </span>
      </div>
      <div class="mbr-navbar__hamburger mbr-hamburger">
        <span class="mbr-hamburger__line">
        </span>
      </div>
      <div class="mbr-navbar__column mbr-navbar__menu">
        <nav class="mbr-navbar__menu-box mbr-navbar__menu-box--inline-right">
          <div class="mbr-navbar__column">
            <ul class="mbr-navbar__items mbr-navbar__items--right mbr-buttons mbr-buttons--freeze mbr-buttons--right btn-decorator mbr-buttons--active">
              <li class="mbr-navbar__item">
                <p class="mbr-buttons__link btn text-white">Welcome {{ currentUser() }}
                </p>
              <li class="mbr-navbar__item">
                <a class="mbr-buttons__link btn text-white" ui-sref="mlb">MLB
                </a>
              <li class="mbr-navbar__item">
                <a class="mbr-buttons__link btn text-white" ui-sref="nhl">NHL
                </a>
              <li class="mbr-navbar__item">
                <a class="mbr-buttons__link btn text-white" ui-sref="nfl">NFL
                </a>
              <li class="mbr-navbar__item">
                <a class="mbr-buttons__link btn text-white" ui-sref="nba">NBA
                </a>
              <li class="mbr-navbar__item">
                <a class="mbr-buttons__link btn text-white" ui-sref="dashboard">Dashboard
                </a>
              </li>
              <li class="mbr-navbar__item" ng-hide="isLoggedIn()">
                <a class="mbr-buttons__link btn text-white" ui-sref="register">Register
                </a>
              </li>
              <li class="mbr-navbar__item" ng-hide="isLoggedIn()">
                <a class="mbr-buttons__link btn text-white" ui-sref="login">Login
                </a>
              </li>
              <li class="mbr-navbar__item" ng-show="isLoggedIn()">
                <a class="mbr-buttons__link btn text-white" href="" ng-click="logOut()">Log Out
                </a>
              </li>
            </ul>
          </div>
          <div class="mbr-navbar__column">
            <ul class="mbr-navbar__items mbr-navbar__items--right mbr-buttons mbr-buttons--freeze mbr-buttons--right btn-inverse mbr-buttons--active">
              <li class="mbr-navbar__item">
                <a class="mbr-buttons__btn btn btn-default" ui-sref="contests">Contests
                </a>
              </li>
            </ul>
          </div>
        </nav>
      </div>
    </div>
  </div>
</div>
</section>
